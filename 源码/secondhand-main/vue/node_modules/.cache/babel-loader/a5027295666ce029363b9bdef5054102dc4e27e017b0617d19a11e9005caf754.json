{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: \"Manager\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      isCollapse: false,\n      isFullScreen: false\n    };\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push('/login');\n    }\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}');\n    },\n    goToPerson() {\n      if (this.user.role === 'ADMIN') {\n        this.$router.push('/adminPerson');\n      }\n    },\n    logout() {\n      this.$confirm('确定要退出登录吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        localStorage.removeItem('xm-user');\n        this.$router.push('/login');\n        this.$message({\n          type: 'success',\n          message: '退出成功!'\n        });\n      }).catch(() => {});\n    },\n    toggleCollapse() {\n      this.isCollapse = !this.isCollapse;\n    },\n    toggleFullScreen() {\n      if (!document.fullscreenElement) {\n        document.documentElement.requestFullscreen();\n      } else {\n        if (document.exitFullscreen) {\n          document.exitFullscreen();\n        }\n      }\n    },\n    refreshPage() {\n      this.$router.go(0);\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","JSON","parse","localStorage","getItem","isCollapse","isFullScreen","created","id","$router","push","methods","updateUser","goToPerson","role","logout","$confirm","confirmButtonText","cancelButtonText","type","then","removeItem","$message","message","catch","toggleCollapse","toggleFullScreen","document","fullscreenElement","documentElement","requestFullscreen","exitFullscreen","refreshPage","go"],"sources":["src/views/Manager.vue"],"sourcesContent":["<template>\n  <div class=\"manager-container\">\n    <!--  头部  -->\n    <div class=\"manager-header\">\n      <div class=\"manager-header-left\">\n        <img src=\"@/assets/imgs/logo.png\" class=\"logo\" />\n        <div class=\"title\">前湖集市后台管理</div>\n      </div>\n\n      <div class=\"manager-header-center\">\n        <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n          <el-breadcrumb-item :to=\"{ path: '/' }\">首页</el-breadcrumb-item>\n          <el-breadcrumb-item :to=\"{ path: $route.path }\">{{ $route.meta.name }}</el-breadcrumb-item>\n        </el-breadcrumb>\n      </div>\n\n      <div class=\"manager-header-right\">\n        <el-tooltip content=\"全屏\" placement=\"bottom\">\n          <i class=\"el-icon-full-screen\" @click=\"toggleFullScreen\"></i>\n        </el-tooltip>\n        <el-tooltip content=\"刷新\" placement=\"bottom\">\n          <i class=\"el-icon-refresh\" @click=\"refreshPage\"></i>\n        </el-tooltip>\n        <el-dropdown placement=\"bottom\" trigger=\"click\">\n          <div class=\"avatar\">\n            <img :src=\"user.avatar || 'https://cube.elemecdn.com/3/7c/3ea6beec64369c2642b92c6726f1epng.png'\" />\n            <div class=\"user-info\">\n              <div class=\"name\">{{ user.name }}</div>\n              <div class=\"role\">{{ user.role === 'ADMIN' ? '管理员' : '普通用户' }}</div>\n            </div>\n          </div>\n          <el-dropdown-menu slot=\"dropdown\">\n            <el-dropdown-item @click.native=\"goToPerson\">\n              <i class=\"el-icon-user\"></i> 个人信息\n            </el-dropdown-item>\n            <el-dropdown-item @click.native=\"$router.push('/password')\">\n              <i class=\"el-icon-lock\"></i> 修改密码\n            </el-dropdown-item>\n            <el-dropdown-item divided @click.native=\"logout\">\n              <i class=\"el-icon-switch-button\"></i> 退出登录\n            </el-dropdown-item>\n          </el-dropdown-menu>\n        </el-dropdown>\n      </div>\n    </div>\n\n    <!--  主体  -->\n    <div class=\"manager-main\">\n      <!--  侧边栏  -->\n      <div class=\"manager-main-left\" :class=\"{ 'is-collapse': isCollapse }\">\n        <div class=\"collapse-btn\" @click=\"toggleCollapse\">\n          <i :class=\"isCollapse ? 'el-icon-s-unfold' : 'el-icon-s-fold'\"></i>\n        </div>\n        <el-menu \n          :default-openeds=\"['info', 'user']\" \n          router \n          style=\"border: none\" \n          :default-active=\"$route.path\"\n          :collapse=\"isCollapse\"\n          class=\"el-menu-vertical\">\n          <el-menu-item index=\"/home\">\n            <i class=\"el-icon-s-home\"></i>\n            <span slot=\"title\">系统首页</span>\n          </el-menu-item>\n          <el-submenu index=\"info\">\n            <template slot=\"title\">\n              <i class=\"el-icon-menu\"></i>\n              <span>信息管理</span>\n            </template>\n            <el-menu-item index=\"/category\">\n              <i class=\"el-icon-folder\"></i>\n              <span>分类信息</span>\n            </el-menu-item>\n            <el-menu-item index=\"/goods\">\n              <i class=\"el-icon-goods\"></i>\n              <span>商品信息</span>\n            </el-menu-item>\n            <el-menu-item index=\"/circles\">\n              <i class=\"el-icon-circle-plus\"></i>\n              <span>圈子信息</span>\n            </el-menu-item>\n            <el-menu-item index=\"/posts\">\n              <i class=\"el-icon-document\"></i>\n              <span>社区帖子</span>\n            </el-menu-item>\n            <el-menu-item index=\"/help\">\n              <i class=\"el-icon-question\"></i>\n              <span>求助信息</span>\n            </el-menu-item>\n            <el-menu-item index=\"/address\">\n              <i class=\"el-icon-location\"></i>\n              <span>收货地址</span>\n            </el-menu-item>\n            <el-menu-item index=\"/feedback\">\n              <i class=\"el-icon-chat-dot-round\"></i>\n              <span>用户反馈</span>\n            </el-menu-item>\n            <el-menu-item index=\"/notice\">\n              <i class=\"el-icon-bell\"></i>\n              <span>公告信息</span>\n            </el-menu-item>\n            <el-menu-item index=\"/comment\">\n              <i class=\"el-icon-chat-line-round\"></i>\n              <span>评论信息</span>\n            </el-menu-item>\n          </el-submenu>\n\n          <el-submenu index=\"user\">\n            <template slot=\"title\">\n              <i class=\"el-icon-user\"></i>\n              <span>用户管理</span>\n            </template>\n            <el-menu-item index=\"/admin\">\n              <i class=\"el-icon-s-custom\"></i>\n              <span>管理员信息</span>\n            </el-menu-item>\n            <el-menu-item index=\"/user\">\n              <i class=\"el-icon-user\"></i>\n              <span>用户信息</span>\n            </el-menu-item>\n          </el-submenu>\n        </el-menu>\n      </div>\n\n      <!--  数据表格  -->\n      <div class=\"manager-main-right\">\n        <router-view @update:user=\"updateUser\" />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"Manager\",\n  data() {\n    return {\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      isCollapse: false,\n      isFullScreen: false\n    }\n  },\n  created() {\n    if (!this.user.id) {\n      this.$router.push('/login')\n    }\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')\n    },\n    goToPerson() {\n      if (this.user.role === 'ADMIN') {\n        this.$router.push('/adminPerson')\n      }\n    },\n    logout() {\n      this.$confirm('确定要退出登录吗?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        localStorage.removeItem('xm-user')\n        this.$router.push('/login')\n        this.$message({\n          type: 'success',\n          message: '退出成功!'\n        })\n      }).catch(() => {})\n    },\n    toggleCollapse() {\n      this.isCollapse = !this.isCollapse\n    },\n    toggleFullScreen() {\n      if (!document.fullscreenElement) {\n        document.documentElement.requestFullscreen()\n      } else {\n        if (document.exitFullscreen) {\n          document.exitFullscreen()\n        }\n      }\n    },\n    refreshPage() {\n      this.$router.go(0)\n    }\n  }\n}\n</script>\n\n<style scoped>\n@import \"@/assets/css/manager.css\";\n\n.manager-header-right i {\n  font-size: 20px;\n  color: #fff;\n  margin-right: 20px;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.manager-header-right i:hover {\n  color: rgba(255, 255, 255, 0.8);\n  transform: scale(1.1);\n}\n\n.user-info {\n  margin-left: 8px;\n  text-align: left;\n}\n\n.user-info .name {\n  font-size: 14px;\n  font-weight: 500;\n}\n\n.user-info .role {\n  font-size: 12px;\n  color: rgba(255, 255, 255, 0.7);\n}\n\n.collapse-btn {\n  height: 40px;\n  line-height: 40px;\n  text-align: center;\n  color: #666;\n  cursor: pointer;\n  transition: all 0.3s;\n}\n\n.collapse-btn:hover {\n  background-color: #f0f7ff;\n  color: #1890ff;\n}\n\n.collapse-btn i {\n  font-size: 20px;\n}\n\n.el-menu-vertical:not(.el-menu--collapse) {\n  width: 240px;\n}\n\n.manager-main-left.is-collapse {\n  width: 64px;\n}\n\n.el-dropdown-menu__item i {\n  margin-right: 8px;\n}\n</style>"],"mappings":";AAqIA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,UAAA;MACAC,YAAA;IACA;EACA;EACAC,QAAA;IACA,UAAAP,IAAA,CAAAQ,EAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAZ,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;IACAS,WAAA;MACA,SAAAb,IAAA,CAAAc,IAAA;QACA,KAAAL,OAAA,CAAAC,IAAA;MACA;IACA;IACAK,OAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAC,IAAA;QACAjB,YAAA,CAAAkB,UAAA;QACA,KAAAZ,OAAA,CAAAC,IAAA;QACA,KAAAY,QAAA;UACAH,IAAA;UACAI,OAAA;QACA;MACA,GAAAC,KAAA;IACA;IACAC,eAAA;MACA,KAAApB,UAAA,SAAAA,UAAA;IACA;IACAqB,iBAAA;MACA,KAAAC,QAAA,CAAAC,iBAAA;QACAD,QAAA,CAAAE,eAAA,CAAAC,iBAAA;MACA;QACA,IAAAH,QAAA,CAAAI,cAAA;UACAJ,QAAA,CAAAI,cAAA;QACA;MACA;IACA;IACAC,YAAA;MACA,KAAAvB,OAAA,CAAAwB,EAAA;IACA;EACA;AACA"},"metad