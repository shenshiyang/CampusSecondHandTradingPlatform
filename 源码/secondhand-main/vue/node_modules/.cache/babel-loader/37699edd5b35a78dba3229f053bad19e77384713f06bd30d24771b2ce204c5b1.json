{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\n// import Footer from \"@/components/Footer\";  // ✅ 已注释\n\nexport default {\n  name: \"FrontLayout\",\n  // components: { Footer },  // ✅ 已注释\n  data() {\n    return {\n      notice: [],\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\n      menus: [{\n        text: '热卖专区',\n        path: '/front/home'\n      }, {\n        text: '社区广场',\n        path: '/front/posts'\n      }, {\n        text: '求购专区',\n        path: '/front/help'\n      }, {\n        text: '系统公告',\n        path: '/front/notice'\n      }, {\n        text: '留言反馈',\n        path: '/front/feedback'\n      }]\n    };\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}');\n    },\n    logout() {\n      localStorage.removeItem(\"xm-user\");\n      this.$router.push(\"/login\");\n    }\n  }\n};","map":{"version":3,"names":["name","data","notice","user","JSON","parse","localStorage","getItem","menus","text","path","methods","updateUser","logout","removeItem","$router","push"],"sources":["src/views/Front.vue"],"sourcesContent":["<template>\n  <div style=\"background-color: #f8f8f8; min-height: 100vh\">\n    <!--头部-->\n    <div class=\"front-header\">\n      <a href=\"/front/home\">\n        <div class=\"front-header-left\">\n          <img src=\"@/assets/imgs/logo.png\" alt=\"\">\n          <div class=\"title\">校园二手平台</div>\n        </div>\n      </a>\n      <div class=\"front-header-center\">\n        <div @click=\"$router.push(item.path)\" class=\"menu\" v-for=\"item in menus\" :key=\"item.path\"\n             :class=\"{'menu-active' : item.path === $route.path }\">{{ item.text }}</div>\n      </div>\n      <div>\n        <span @click=\"$router.push('/front/chat')\" style=\"font-size: 16px; color: white; cursor: pointer\">\n          <i class=\"el-icon-chat-dot-round\"></i> 聊天消息\n        </span>\n      </div>\n      <div class=\"front-header-right\">\n        <div v-if=\"!user.username\">\n          <el-button @click=\"$router.push('/login')\">登录</el-button>\n          <el-button @click=\"$router.push('/register')\">注册</el-button>\n        </div>\n        <div v-else>\n          <el-dropdown>\n            <div class=\"front-header-dropdown\">\n              <img :src=\"user.avatar\" alt=\"\" style=\"border-radius: 50%\">\n              <div style=\"margin-left: 10px; color: #eee; cursor: pointer\">\n                <span>{{ user.name }}</span><i class=\"el-icon-arrow-down\" style=\"margin-left: 5px\"></i>\n              </div>\n            </div>\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item><div @click=\"$router.push('/front/orders')\">我的订单</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/goods')\">我的商品</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/userPosts')\">我的发帖</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/userHelp')\">我的求购</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/userFeedback')\">我的反馈</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/address')\">我的地址</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/person')\">个人信息</div></el-dropdown-item>\n              <el-dropdown-item><div @click=\"$router.push('/front/collect')\">我的收藏</div></el-dropdown-item>\n              <el-dropdown-item><div style=\"text-decoration: none\" @click=\"logout\">退出登录</div></el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n      </div>\n    </div>\n\n    <!-- 主体 -->\n    <div class=\"main-body\">\n      <router-view ref=\"child\" @update:user=\"updateUser\" />\n    </div>\n\n    <!-- <Footer />  页脚已注释 -->\n\n  </div>\n</template>\n\n<script>\n// import Footer from \"@/components/Footer\";  // ✅ 已注释\n\nexport default {\n  name: \"FrontLayout\",\n  // components: { Footer },  // ✅ 已注释\n  data () {\n    return {\n      notice: [],\n      user: JSON.parse(localStorage.getItem(\"xm-user\") || '{}'),\n      menus: [\n        { text: '热卖专区', path: '/front/home' },\n        { text: '社区广场', path: '/front/posts' },\n        { text: '求购专区', path: '/front/help' },\n        { text: '系统公告', path: '/front/notice' },\n        { text: '留言反馈', path: '/front/feedback' },\n      ]\n    }\n  },\n  methods: {\n    updateUser() {\n      this.user = JSON.parse(localStorage.getItem('xm-user') || '{}')\n    },\n    logout() {\n      localStorage.removeItem(\"xm-user\");\n      this.$router.push(\"/login\");\n    },\n  }\n}\n</script>\n\n<style scoped>\n@import \"@/assets/css/front.css\";\n\n.menu {\n  color: #eee;\n  font-size: 16px;\n  padding: 0 20px;\n  cursor: pointer;\n}\n.menu:hover {\n  color: orange;\n}\n.menu-active {\n  color: orange;\n}\n</style>\n"],"mappings":";AA2DA;;AAEA;EACAA,IAAA;EACA;EACAC,KAAA;IACA;MACAC,MAAA;MACAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;MACAC,KAAA,GACA;QAAAC,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,IAAA;MAAA;IAEA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAT,IAAA,GAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;IACAM,OAAA;MACAP,YAAA,CAAAQ,UAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}